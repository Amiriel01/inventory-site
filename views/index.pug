extends layout

block content
  //- h1= title
  //- p Welcome to #{title}, an inventory site. 

  h1 Welcome to this tea inventory site.

  h2= title

  ul 
    each category in category_list 
      li 
        a(href=category.url) #{category.name}
        | (#{category.name})
    else 
      li There are no more categories.


